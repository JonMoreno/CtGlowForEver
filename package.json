    "cross-env": "^5.1.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "laravel-mix": "^1.7.2",
    "lodash": "^4.17.10",
    "vue": "^2.5.16"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "animate.css": "^3.6.1",
    "fullcalendar": "^3.9.0",
    "implement-js": "0.0.25",
    "moment": "^2.22.1",
